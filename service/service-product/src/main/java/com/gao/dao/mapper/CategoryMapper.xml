<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gao.dao.CategoryMapper">
	<!--开启二级缓存-->
	<!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

	<resultMap id="BaseResultMap" type="com.gao.entity.po.Category">
		<id column="id" property="id" />
		<result column="category_name" property="categoryName" />
		<result column="parent_id" property="parentId" />
		<result column="sort" property="sort" />
		<result column="gmt_create" property="gmtCreate" />
		<result column="gmt_update" property="gmtUpdate" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, category_name AS categoryName, parent_id AS parentId, sort, gmt_create AS gmtCreate, gmt_update AS gmtUpdate
    </sql>
</mapper>
